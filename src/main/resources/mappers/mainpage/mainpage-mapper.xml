<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ss.sns.main.mapper.MainPageMapper">
	<select id="searchCity" parameterType="string" resultType="CityDTO">
		select * from city where city_name LIKE '%' || #{searchText} || '%'
	</select>
	<select id="selectAllCity" resultType="CityDTO">
	select * from city
	</select>
</mapper>